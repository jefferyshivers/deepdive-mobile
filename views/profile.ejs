<!DOCTYPE html>
<html>

  <head>
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width,height=device-height initial-scale=1">  
    <link rel='stylesheet' href='/stylesheets/all.css'/>
    <link rel='stylesheet' href='/stylesheets/top-nav.css'/>     
    <link rel='stylesheet' href='/stylesheets/profile.css'/>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Noto+Sans"/>
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons"/>
    <link href="https://fonts.googleapis.com/css?family=Rajdhani" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro" rel="stylesheet">  
    <link rel="icon" type="image/png" sizes="16x16" href="../images/icons/deepdive-icon.png">  
  </head>

<!-- Jason's firebase Key -->
<script src="https://www.gstatic.com/firebasejs/4.1.3/firebase.js"></script>
<script src="/javascripts/app/db/config.js"></script>
<!-- /Jason's firebase Key -->

<script src="/javascripts/app/db/auth/bundle.js"></script>
<script src="/javascripts/app/db/auth/email.js"></script>

  <body>

<!-- Facebook SDK, the following should be right after the <body> tag -->
<script src="/javascripts/app/db/auth/facebookSDK.js"></script> 


	<!-- top nav -->
	  <div class="top-nav">
	  	<div class="left-gutter"></div>
	  	<div class="full-container">




        <div class="search-container">

          <div class="search-icon">
            <i class="material-icon">search</i>
          </div>

        </div>





        <div class="add-and-user">
          <div class="add-content">
            <i class="material-icon">add_circle_outline</i>
          </div>
          <div class="notifications">
            <i class="material-icon">notifications_none</i>
          </div>            
          <div class="user">
            <i class="material-icon">person</i>
          </div>
        </div>





	  	</div>
	  </div>







  <div class="top-nav-searchover">
    <div class="close">
      <i class="material-icon">close</i>
    </div>
    <div class="search-enclosure">
      <input type="text" placeholder="Search" class="new-query" id="new-query">
    </div>
  </div>


  

	<!-- main body -->
	  <div class="main">
	  	<div class="left-gutter"></div>
	  	<div class="body">
        <div class="left-nav">
          <div class="top">
            <div class="user-icon-container">
              <div class="user-icon"></div>
            </div>
            <div class="user-name" id="username"></div>
            <div class="bottom-sep"></div>
          </div>
          <div class="rest">
            

            <div class="following">
              <div class="key">Following</div>
              <div class="value" id="following"></div>
            </div>
            <div class="followers">
              <div class="key">Followers</div>
              <div class="value" id="followers"></div>
            </div>
            <div class="likes">
              <div class="key">Likes</div>
              <div class="value" id="likes"></div>
            </div>



            <div class="new-bottom-sep"></div>



            <!--  -->
            <div class = "artistblock">
              <div class = "nameinblock" id = "name-one"></div>
              <div class = "squareblock">
                <div class = "realsquare">
                  <div class="percentage" id="perc_one"></div>
                </div>
                <div class="number" id="numone">100</div>
              </div>
            </div>
            <div class = "artistblock">
              <div class = "nameinblock" id = "name-two"></div>
              <div class = "squareblock">
                <div class = "realsquare">
                  <div class="percentage" id="perc_two"></div>
                </div>
                <div class="number" id="numtwo"></div>
              </div>
            </div>
            <div class = "artistblock">
              <div class = "nameinblock" id = "name-three"></div>
              <div class = "squareblock">
                <div class = "realsquare">
                  <div class="percentage" id="perc_three"></div>
                </div>
                 <div class="number" id="numthree"></div>
              </div>
            </div>
          </div>
        </div>
        <div class="inner-body">
          <div class="top">
            <div class="button-container">
              <div class="button" id="collections">
                Collections
              </div>              
              <div class="button" id="contributions">
                Submissions
              </div>            
            </div>
          </div>
          <!-- <div class="mid"></div> -->
          <div class="contents">
          </div>
        </div>  
      </div>
	  </div>



  </body>




	<!-- scripts -->
	  <script src="/javascripts/lib/jquery-3.2.1.min.js"></script>
		<script src="/javascripts/app/ui/top-nav.js"></script>
		<script src="/javascripts/app/ui/main.js"></script>
    <script src='/javascripts/app/ui/search.js'></script>
    <script src='/javascripts/app/ui/add.js'></script>
    <script src='/javascripts/app/ui/signup.js'></script>
    <script src='/javascripts/app/ui/login.js'></script>
    <script src='/javascripts/app/loadpage.js'></script>
    <script src='/javascripts/app/db/vote.js'></script>
    <script src='/javascripts/app/db/follow.js'></script>
    <script src='/javascripts/app/ui/profile.js'></script>
    <script src='/javascripts/app/db/like.js'></script>    
    <script src='/javascripts/app/db/loadcollection.js'></script>
    <script src='/javascripts/app/ui/popups.js'></script>
    <script>
      loadpage()
      load_profile_page()
    </script>
    
  </body>

</html>
